{
  "name": "@netdata/charts",
  "version": "0.3.0",
  "description": "Netdata frontend SDK and chart utilities",
  "main": "index.js",
  "module": "es6/index.js",
  "jsnext:main": "es6/index.js",
  "repository": "git@github.com:netdata/charts.git",
  "author": "Netdata",
  "license": "GPL-3.0",
  "private": false,
  "files": [
    "/dist"
  ],
  "scripts": {
    "build:cjs": "babel src --out-dir dist --copy-files --ignore **/*.stories.js --ignore ./src/fixtures",
    "build:es6": "BABEL_ENV=es6 babel src --out-dir dist/es6 --copy-files --ignore **/*.stories.js --ignore ./src/fixtures",
    "build:umd": "NODE_ENV=production webpack",
    "build": "yarn build:umd && yarn build:cjs && yarn build:es6",
    "test": "jest --config ./jest/config.js",
    "lint": "eslint './src/**/*.js'",
    "clean": "rimraf lib dist es coverage",
    "prepublish": "yarn clean && yarn build",
    "start": "start-storybook -p 6006",
    "deploy-storybook": "storybook-to-ghpages",
    "cp-cloud": "cp -R ./dist ../cloud-frontend/node_modules/@netdata/charts/",
    "to-cloud": "yarn build && yarn cp-cloud"
  },
  "keywords": [
    "netdata",
    "sdk",
    "frontend",
    "charts"
  ],
  "peerDependencies": {
    "@netdata/netdata-ui": "^1.9.6",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-transform-modules-umd": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@netdata/netdata-ui": "^1.9.6",
    "@storybook/react": "^6.4.18",
    "@storybook/storybook-deployer": "^2.8.10",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^2.0.2",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.3.1",
    "prettier": "^2.5.1",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "storybook": "^6.4.18",
    "styled-components": "^5.3.3",
    "svg-sprite-loader": "^6.0.11",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-plugin": "^1.0.5"
  },
  "dependencies": {
    "@netdata/dygraphs": "^2.1.1",
    "d3-scale": "^4.0.2",
    "easy-pie-chart": "^2.1.7",
    "gaugeJS": "^1.3.7",
    "md5": "^2.3.0",
    "throttle-debounce": "^3.0.1"
  }
}

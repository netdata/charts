{
  "name": "@netdata/charts",
  "version": "3.0.0-16",
  "description": "Netdata frontend SDK and chart utilities",
  "main": "index.js",
  "module": "es6/index.js",
  "jsnext:main": "es6/index.js",
  "repository": "git@github.com:netdata/charts.git",
  "author": "Netdata",
  "license": "GPL-3.0",
  "private": false,
  "files": [
    "/dist"
  ],
  "scripts": {
    "build:cjs": "babel src --out-dir dist --copy-files --ignore **/*.stories.js --ignore ./src/fixtures",
    "build:es6": "BABEL_ENV=es6 babel src --out-dir dist/es6 --copy-files --ignore **/*.stories.js --ignore ./src/fixtures",
    "build:umd": "NODE_ENV=production webpack",
    "build": "yarn build:umd && yarn build:cjs && yarn build:es6",
    "test": "jest --config ./jest/config.js",
    "lint": "eslint './src/**/*.js'",
    "clean": "rimraf lib dist es coverage",
    "prepublish": "yarn clean && yarn build",
    "start": "start-storybook -p 6006",
    "deploy-storybook": "storybook-to-ghpages",
    "cp-cloud": "cp -R ./dist ../cloud-frontend/node_modules/@netdata/charts/",
    "to-cloud": "yarn build && yarn cp-cloud"
  },
  "keywords": [
    "netdata",
    "sdk",
    "frontend",
    "charts"
  ],
  "peerDependencies": {
    "@netdata/netdata-ui": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-modules-umd": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@netdata/netdata-ui": "^2.13.1",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/storybook-deployer": "^2.8.16",
    "babel-loader": "^9.1.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-styled-components": "^2.0.7",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.36.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.4.0",
    "node-sass": "^8.0.0",
    "prettier": "^2.8.4",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0",
    "rimraf": "^4.4.0",
    "sass-loader": "^13.2.0",
    "storybook": "^6.5.16",
    "styled-components": "^5.3.9",
    "svg-sprite-loader": "^6.0.11",
    "svgo-loader": "^4.0.0",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1",
    "webpack-plugin": "^1.0.5"
  },
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "d3-scale": "^4.0.2",
    "dygraphs": "^2.2.1",
    "easy-pie-chart": "^2.1.7",
    "gaugeJS": "^1.3.7",
    "md5": "^2.3.0",
    "throttle-debounce": "^5.0.0"
  }
}
